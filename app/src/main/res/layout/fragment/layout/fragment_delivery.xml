<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/app_primary_color"
    tools:context="com.example.administrator.jiayan_project.ui.fragment.mine.DeliveryFragment">
    <com.qmuiteam.qmui.widget.QMUITopBar
        app:qmui_topbar_bg_color="@color/qmui_config_color_transparent"
        app:qmui_topbar_need_separator="false"
        app:qmui_topbar_title_color="@color/white"
        android:id="@+id/mtopbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/qmui_topbar_height"/>
    <!--<android.support.v7.widget.RecyclerView-->
        <!--android:background="@color/white"-->
        <!--android:layout_marginTop="?attr/qmui_topbar_height"-->
        <!--android:id="@+id/recyclerview"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"/>-->
    <ListView
        android:background="#f7f6f7"
        android:layout_marginTop="?attr/qmui_topbar_height"
        android:id="@+id/lv_address"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:divider="@null"
        android:dividerHeight="8dp"
        android:scrollbars="none" />

</FrameLayout>
