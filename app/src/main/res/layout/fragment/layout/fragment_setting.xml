<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="?attr/app_primary_color"
    tools:context="com.example.administrator.jiayan_project.ui.fragment.SettingFragment">
    <com.qmuiteam.qmui.widget.QMUITopBar
        app:qmui_topbar_bg_color="@color/qmui_config_color_transparent"
        app:qmui_topbar_need_separator="false"
        app:qmui_topbar_title_color="@color/white"
        android:id="@+id/mtopbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/qmui_topbar_height"/>
    <ScrollView
        android:layout_marginTop="?attr/qmui_topbar_height"
        android:background="@color/qmui_config_color_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <com.qmuiteam.qmui.widget.grouplist.QMUIGroupListView

        android:id="@+id/groupListView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    </ScrollView>
</FrameLayout>
