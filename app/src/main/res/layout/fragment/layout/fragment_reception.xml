<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/app_color_blue"
    tools:context="com.example.administrator.jiayan_project.ui.fragment.chef_service.ReceptionFragment">
    <com.qmuiteam.qmui.widget.QMUITopBar
        app:qmui_topbar_bg_color="@color/qmui_config_color_transparent"
        app:qmui_topbar_need_separator="false"
        app:qmui_topbar_title_color="@color/white"
        android:id="@+id/mtopbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/qmui_topbar_height"/>
    <com.example.administrator.jiayan_project.utils.weight.FatRecyclerview
        android:layout_marginTop="?attr/qmui_topbar_height"
        android:id="@+id/recycler"
        android:background="@color/white"
        android:overScrollMode="never"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</FrameLayout>
